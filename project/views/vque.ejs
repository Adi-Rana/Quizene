<!DOCTYPE html>
<html>
<head>
    <% include partials/teacher/head %>
</head>
<style>
table {
  border-collapse: collapse;
  width: 100%;
}

th, td {
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {background-color: #f2f2f2;}
</style>
<body class="container">
<header>
    <% include partials/teacher/header %>
</header>
 <% include partials/teacher/side %>

 <div class="col-sm-8">    
        <div class="jumbotron">
		<h2> Questions List </h2>
		<table>
		<tr>
		<th>Test</th>
		<th>Questions</th>
		<th> </th>
		
		</tr>
		<% cquestions.forEach(function(cquestion) { %>
		<tr>
		<td><%= cquestion.testName %></td>
		<td>
		
		<h4><%= cquestion.que %><h4>
		
		</td>
		
		<td><form action="/cquestion/<%= cquestion.que%>/delete" method="post">
		<div class="form-group">
		
		<button type="submit" class="btn btn-danger">Remove</button>
		</div>
		</form></td>
		</tr>
		<% }); %>
		</table>
		</div>
		</div>
		







<footer>
    <% include partials/teacher/footer %>
</footer>
</body>
</html>